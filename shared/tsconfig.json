{
	"$schema": "https://json.schemastore.org/tsconfig.json",
	"extends": "../tsconfig.json",
	"compilerOptions": {
		"paths": {
			"#/*": [
				"./src/*"
			]
		},
		"types": [
			"vite/client",
			"fvtt-types/lenient"
		],
		"plugins": [
			{
				"name": "typescript-vue-plugin",
			}
		],
	},
	"include": [
		"**/*.ts",
	]
}
